def solution(cacheSize, cities):
    cache = []
    answer = 0
    for c in cities:
        c = c.upper()
        if c in cache:
            answer += 1
            cache.remove(c)
            cache.append(c)
        else:
            if cacheSize > 0:
                if len(cache) == cacheSize:
                    cache.pop(0)
                cache.append(c)
            answer += 5
    return answer